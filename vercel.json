{
  "version": 2,
  "functions": {
    "api/wsgi.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "certifly.settings"
  }
}