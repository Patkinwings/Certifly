{
  "version": 2,
  "builds": [
    {
      "src": "certifly/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "certifly/wsgi.py"
    }
  ]
}